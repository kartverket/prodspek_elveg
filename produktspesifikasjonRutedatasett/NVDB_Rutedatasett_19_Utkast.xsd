  <simpleType name="LandbruksvegklasseType">
    <annotation>
      <documentation>Landbruksmyndighetene sin inndeling av landbruksveger, ut ifra støtteordninger.</documentation>
      <appinfo>
        <sc:taggedValue tag="NVDB_ID">9339</sc:taggedValue>
      </appinfo>
    </annotation>
    <union memberTypes="app:LandbruksvegklasseEnumerationType app:LandbruksvegklasseOtherType"/>
